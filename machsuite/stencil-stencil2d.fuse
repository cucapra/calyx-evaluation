decl orig: bit<32>[128][64];
decl sol: bit<32>[128][64];
decl filter: bit<32>[3][3];

for (let r = 0..126) {
  for (let c = 0..62) {
    let temp: bit<32> = 0;
    for (let k1 = 0..3) {
      let temp_2: bit<32> = 0;
      for (let k2 = 0..3) {
        let mul = filter[k1][k2] * orig[r+k1][c+k2];
      } combine {
        temp_2 += mul;
      }
    } combine {
      temp += temp_2;
    }
    sol[r][c] := temp;
  }
}
